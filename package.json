{
  "name": "portfolio",
  "description": "This is my developer portfolio built with NextJS, React, Node back-end, and Express server.",
  "main": "server.js",
  "version": "0.1.0",
  "engines": {
    "node": "12.4.0",
    "npm": "6.4.1"
  },
  "private": true,
  "devDependencies": {
    "browser-sync": "^2.24.7",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body": "^5.1.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-request-id": "^1.4.1",
    "express-session": "^1.15.6",
    "express-winston": "^3.0.0",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "sequelize": "^4.37.7",
    "supertest": "^3.1.0",
    "winston": "^3.1.0"
  },
  "scripts": {
    "server": "node server.js",
    "client": "cd client && npm run start",
    "start": "concurrently \"pm2-runtime start ecosystem.config.js --env production\" \"npm run client\"",
    "dev-start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "cd client && npm run build",
    "deploy": "yarn build && git add . && git commit -m \"Building for production\" && git push heroku master",
    "test": "mocha test/test.js",
    "eject": "react-scripts eject",
    "installDeps": "yarn && cd client && yarn",
    "sass": "sass --watch src/styles/scss:src/styles/css"
  },
  "repository": "https://github.com/WillBlake01/react-portfolio.git",
  "author": "William Blake <willblakebooking@gmail.com>",
  "license": "MIT"
}
